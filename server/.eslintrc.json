{
    "env": {
        "es2021": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "plugins": [
        "prettier",
        "eslint-plugin-prettier"
    ],
    "rules": {
        "prettier/prettier": "error"
    }
}
